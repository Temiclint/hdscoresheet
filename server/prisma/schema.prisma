// generate the client
generator client{
    provider = "prisma-client-js"
}

//data soure
datasource db{
    provider = "mongodb"
    url = env("DATABASE_URL")
}

model User{
    id String @id @default(auto()) @map("_id") @db.ObjectId
    firstname String
    lastname String
    email String @unique
    professionalEmail String @unique
    linkedin String @unique
    upwork String @unique
    facebook String @unique
    twitter String @unique
    funnel String @unique
    medium String @unique
    gohigh String @unique
    phoneNumber String
    // uniqueCode String @unique
    upworkOut Int
    socialPost Int
    socialEngage Int
    JobApp Int
    localOut Int
    intOut Int
    ecommerce Int
    NumOfClients Int
    totalEarnings Int
    totalPoints Int

}

